  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach",
      "type": "cppvsdbg",
      "request": "attach",
      "processId": "${command:pickProcess}"
    },
    {
      "name": "(Debug) Boids",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\Debug\\sample.exe",
      "args": ["data/entities_boids.json"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}\\sample",
      "environment": [],
      "externalConsole": true,
      "preLaunchTask": "build",
      // "visualizerFile": "${workspaceFolder}\\EASTL\\doc\\EASTL.natvis"
      "visualizerFile": "${workspaceFolder}\\sample\\sample.natvis"
    },
    {
      "name": "(Release) Boids",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\Release\\sample.exe",
      "args": ["data/entities_boids.json"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}\\sample",
      "environment": [],
      "externalConsole": true,
      "preLaunchTask": "build release",
      // "visualizerFile": "${workspaceFolder}\\EASTL\\doc\\EASTL.natvis"
      "visualizerFile": "${workspaceFolder}\\sample\\sample.natvis"
    },
    {
      "name": "(Debug) Sample",
      "type": "cppvsdbg",
      "request": "launch",
      // "program": "${workspaceFolder}\\Debug\\sample.exe",
      "program": "${workspaceFolder}\\bin\\sample-dbg.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}\\sample",
      "environment": [],
      "externalConsole": true,
      // "preLaunchTask": "build",
      // "visualizerFile": "${workspaceFolder}\\EASTL\\doc\\EASTL.natvis"
      "visualizerFile": "${workspaceFolder}\\sample\\sample.natvis"
    },
    {

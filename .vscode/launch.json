      "type": "cppvsdbg",
      "request": "launch",
      // "program": "${workspaceFolder}\\Release\\benchmark.exe",
      "program": "${workspaceFolder}\\bin\\ecs-benchmark.exe",
      // "args": [],
      // "args": ["--benchmark_filter=ScriptFor2/2048"],
      "args": ["--benchmark_filter=ScriptForDas/2048"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}\\benchmark",
      "environment": [],
